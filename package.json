{
  "name": "blogm",
  "version": "1.0.0",
  "description": "blog manager using sql",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed && jest --detectOpenHandles",
    "test:coverage": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed && jest --detectOpenHandles --coverage",
    "coveralls": "npm run test:coverage && node scripts/coveralls.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc && copyfiles -u 1 \"src/templates/**/*\" dist/src",
    "start": "node dist/src/server.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo:all",
    "migration:create": "sequelize-cli migration:generate --name",
    "migration:run": "npx sequelize-cli db:migrate",
    "seed:create": "sequelize-cli seed:generate --name",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:ps": "docker-compose ps",
    "docker:restart": "docker-compose restart",
    "docker:stop": "docker-compose stop",
    "rabbitmq:up": "docker-compose up -d rabbitmq",
    "rabbitmq:down": "docker-compose stop rabbitmq",
    "rabbitmq:logs": "docker-compose logs -f rabbitmq",
    "rabbitmq:restart": "docker-compose restart rabbitmq",
    "rabbitmq:remove": "docker-compose rm -f rabbitmq"
  },
  "dependencies": {
    "@types/amqplib": "^0.10.7",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^6.4.17",
    "amqplib": "^0.10.8",
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.7.0",
    "connect-session-sequelize": "^8.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "jwt-redis": "^7.0.3",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.5",
    "nodemon": "^3.1.10",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.7.1",
    "sequelize": "^6.37.7",
    "swagger-ui-express": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "uuid": "^11.1.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.15",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/redis": "^4.0.10",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "babel-register": "^6.26.0",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.1",
    "jest": "^30.0.5",
    "jest-mock-extended": "^4.0.0",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
